{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/_app.page.jsx", "components/UI/CreateLevels/index.jsx", "components/Ckeditor/index.jsx", "components/HtmlParser/index.jsx", "components/UI/UsersList/index.jsx", "components/CarouselCardMain/index.jsx", "components/HeadingWithNav/index.jsx", "components/HeadingWithNav/index.jsx", "components/CardMain/index.jsx", "components/CardMain/index.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
